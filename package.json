{
  "name": "rest-book",
  "displayName": "REST Book",
  "description": "A Postman-like REST API Notebook for sending to and receiving from REST endpoints.",
  "version": "0.2.1",
  "publisher": "tanha",
  "repository": {
    "url": "https://github.com/tanhakabir/rest-book"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "enableProposedApi": true,
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:restbook.notebook",
    "onLanguage:rest-book"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "notebookProvider": [
      {
        "viewType": "restbook.notebook",
        "displayName": "REST Book",
        "priority": "default",
        "selector": [
          {
            "filenamePattern": "*.restbook"
          }
        ]
      }
    ],
    "languages": [
			{
				"id": "rest-book",
				"aliases": [
					"REST Book"
				],
				"filenamePatterns": [
					"*.rest-book.txt"
				]
			}
		]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/user-home": "^2.0.0",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "axios": "^0.21.0",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-dts": "^0.3.1",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "lodash": "^4.17.20",
    "user-home": "^2.0.0"
  }
}
